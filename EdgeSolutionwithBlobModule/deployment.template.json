{
  "$schema-template": "1.0.0",
  "modulesContent": {
    "$edgeAgent": {
      "properties.desired": {
        "schemaVersion": "1.0",
        "runtime": {
          "type": "docker",
          "settings": {
            "loggingOptions": "",
            "minDockerVersion": "v1.25",
            "registryCredentials": {
              "mcr": {
                "address": "mcr.microsoft.com",
                "password": "$CONTAINER_REGISTRY_PASSWORD_mcr",
                "username": "$CONTAINER_REGISTRY_USERNAME_mcr"
              },
              "amlworkspace4142236962": {
                "username": "$CONTAINER_REGISTRY_USERNAME_amlworkspace4142236962",
                "password": "$CONTAINER_REGISTRY_PASSWORD_amlworkspace4142236962",
                "address": "amlworkspace4142236962.azurecr.io"
              }
            }
          }
        },
        "systemModules": {
          "edgeAgent": {
            "type": "docker",
            "settings": {
              "image": "mcr.microsoft.com/azureiotedge-agent:1.0",
              "createOptions": "{}"
            }
          },
          "edgeHub": {
            "type": "docker",
            "settings": {
              "image": "mcr.microsoft.com/azureiotedge-hub:1.0",
              "createOptions": "{\"HostConfig\":{\"PortBindings\":{\"5671/tcp\":[{\"HostPort\":\"5671\"}],\"8883/tcp\":[{\"HostPort\":\"8883\"}],\"443/tcp\":[{\"HostPort\":\"443\"}]}}}"
            },
            "status": "running",
            "restartPolicy": "always"
          }
        },
        "modules": {
          "tempSensor": {
            "type": "docker",
            "settings": {
              "image": "mcr.microsoft.com/azureiotedge-simulated-temperature-sensor:1.0",
              "createOptions": "{}"
            },
            "version": "1.0",
            "status": "running",
            "restartPolicy": "always"
          },
          "blob": {
            "type": "docker",
            "settings": {
              "image": "mcr.microsoft.com/azure-blob-storage:latest",
              "createOptions": "{\"Env\": [\"LOCAL_STORAGE_ACCOUNT_NAME=$STORAGE_ACCOUNT_NAME\",\"LOCAL_STORAGE_ACCOUNT_KEY=$STORAGE_ACCOUNT_KEY\"],\"HostConfig\": {\"Binds\": [\"/srv/containerdata:/blobroot\"], \"PortBindings\": {\"11002/tcp\": [{\"HostPort\":\"11002\"}]}}}"
            },
            "version": "1.0",
            "status": "running",
            "restartPolicy": "always"
          },
          "StoreMessagesToBlob": {
            "type": "docker",
            "settings": {
              "image": "${MODULES.StoreMessagesToBlob}",
              "createOptions": "{}"
            },
            "version": "1.0",
            "status": "running",
            "restartPolicy": "always"
          },
          "UpstreamFromBlob": {
            "type": "docker",
            "settings": {
              "image": "${MODULES.UpstreamFromBlob}",
              "createOptions": "{}"
            },
            "version": "1.0",
            "status": "running",
            "restartPolicy": "always"
          },
          "mvconedge": {
            "type": "docker",
            "settings": {
              "image": "${MODULES.mvc}",
              "createOptions": "{\"HostConfig\":{\"PortBindings\":{\"80/tcp\":[{\"HostPort\":\"8080\"}]}}}"
            },
            "version": "1.0",
            "status": "running",
            "restartPolicy": "always"
          },
          "samodule": {
            "type": "docker",
            "settings": {
              "image": "microsoft/azureiotedge-azure-stream-analytics:1.0.0",
              "createOptions": ""
            },
            "env": {
              "PlanId": {
                "value": "stream-analytics-on-iot-edge"
              }
            },
            "status": "running",
            "restartPolicy": "always",
            "version": "1.0"
          },
          "mongodbmodule": {
            "type": "docker",
            "settings": {
                "image": "mongo:latest",
                "createOptions": "{\"HostConfig\":{\"Binds\":[\"/data/db\"],\"PortBindings\":{\"27017/tcp\":[{\"HostPort\":\"27017\"}]}}}"
            },
            "version": "1.0",
            "status": "running",
            "restartPolicy": "always"
          },
          "anomalydetection": {
            "version": "1.0",
            "type": "docker",
            "status": "running",
            "restartPolicy": "always",
            "settings": {
              "image": "amlworkspace4142236962.azurecr.io/anomalydetection:2",
              "createOptions": {}
            }
          },
          "tempSensorForMongo": {
            "type": "docker",
            "settings": {
              "image": "mcr.microsoft.com/azureiotedge-simulated-temperature-sensor:1.0",
              "createOptions": "{}"
            },
            "version": "1.0",
            "status": "running",
            "restartPolicy": "always"
          },
          "StoreMessagesToMongo": {
            "version": "1.0",
            "type": "docker",
            "status": "running",
            "restartPolicy": "always",
            "settings": {
              "image": "${MODULES.StoreMessagesToMongo}",
              "createOptions": {}
            }            
          }
        }
      }
    },
    "$edgeHub": {
      "properties.desired": {
        "schemaVersion": "1.0",
        "routes": {          
          "sensorToStoreMessagesToBlob": "FROM /messages/modules/tempSensor/outputs/temperatureOutput INTO BrokeredEndpoint(\"/modules/StoreMessagesToBlob/inputs/inputfortempsensor\")",
          "sensorToMachineLearning":"FROM /messages/modules/tempSensor/outputs/temperatureOutput INTO BrokeredEndpoint(\"/modules/anomalydetection/inputs/amlInput\")",
          "mlToStoreMessagesToBlob": "FROM /messages/modules/anomalydetection/outputs/* WHERE $body.anomaly = true INTO BrokeredEndpoint(\"/modules/StoreMessagesToBlob/inputs/inputforml\")",
          "telemetryToAsa": "FROM /messages/modules/tempSensor/outputs/temperatureOutput INTO BrokeredEndpoint(\"/modules/samodule/inputs/temperature\")",
          "alertsToReset": "FROM /messages/modules/samodule/outputs/alert INTO BrokeredEndpoint(\"/modules/tempSensor/inputs/control\")",
          "UpstreamFromBlobToIoTHub": "FROM /messages/modules/UpstreamFromBlob/outputs/output1 INTO $upstream",
          "telemetryToMongo": "FROM /messages/modules/tempSensorForMongo/* INTO BrokeredEndpoint(\"/modules/StoreMessagesToMongo/inputs/input1\")"
        },
        "storeAndForwardConfiguration": {
          "timeToLiveSecs": 15
        }
      }
    },
    "samodule": {
      "properties.desired": {
        "ASAJobInfo": "$ASA_JOB_INFO",
        "ASAJobResourceId": "$ASA_JOB_RESOURCEID",
        "ASAJobEtag": "$ASA_JOB_ETAG",
        "PublishTimestamp": "$PUBLISH_TIMESTAMP"
      }
    }
  }
}